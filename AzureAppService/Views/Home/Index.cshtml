@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <h3>Login!</h3>

    <form>
        <div>
            <label>Email</label>
            <input type="email" />
        </div>
        <div>
            <label>Username</label>
            <input type="text" />
        </div>
        <div>
            <label>Password</label>
            <input type="password"/>
        </div>

        <div>
            <button type="submit" name="Submit form" >Submit</button>
        </div>
    </form>
</div>

